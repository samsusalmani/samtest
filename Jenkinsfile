pipeline {
  agent any
  stages {
    stage("build") {
      steps {
      	echo 'Building now...'	
	sleep 5
      }
    }
    stage("test") {
      steps {
        echo 'Testing now...'
	sleep 5
      }
    }
    stage("deploy") {
      steps {
        echo 'Deploying now...'
	sleep 5
      }
    }
  }
}
